= simple_form_for(@work_log) do |f|
  = f.error_notification
  %fieldset
  	%legend= content_for?(:title) ? yield(:title) : "Untitled"
  	.form_inputs
  		= f.association :task
  		= f.input :start_date
  		= f.input :start_time
  		= f.input :end_date
  		= f.input :end_time
  		= f.input :hours
  	.form-actions
  		= f.button :submit
